    <parent>
        <groupId>org.cuckoo-ra</groupId>
        <artifactId>cuckoo-parent</artifactId>
        <version>1.1.0</version>
    </parent>

    <artifactId>cuckoo-rar</artifactId>
    <packaging>rar</packaging>

    <build>
